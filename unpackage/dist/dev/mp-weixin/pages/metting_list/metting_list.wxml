<view class="page"><image class="logo" src="../../static/logo-3.jpg" mode="widthFix"></image><view class="add"><image class="icon" src="../../static/icon-17.png" mode="widthFix"></image><text>创建会议</text></view><block wx:for="{{list}}" wx:for-item="one" wx:for-index="__i0__" wx:key="date"><view><view class="list-title">{{one.date}}</view><block wx:for="{{one.list}}" wx:for-item="metting" wx:for-index="__i1__" wx:key="id"><view class="item"><view class="header"><view class="left"><block wx:if="{{metting.type=='线上会议'}}"><image class="icon" src="../../static/icon-11.png" mode="widthFix"></image></block><block wx:if="{{metting.type=='线下会议'}}"><image class="icon" src="../../static/icon-11.png" mode="widthFix"></image></block><text>{{metting.type}}</text><text class="{{[metting.status=='未开始'?'blue':'red']}}">{{"（"+metting.status+")"}}</text></view><view class="right"><text>编辑</text></view></view><view class="content"><view class="title">{{''+metting.title+"\n\t\t\t\t\t"+metting.name+''}}</view><view class="attr"><view class="timer"><image class="icon" src="../../static/icon-14.png" mode="widthFix"></image><text>{{metting.start+"~"+metting.end}}</text></view><view class="creator"><image class="icon" src="../../static/icon-15.png" mode="widthFix"></image><view>{{metting.name+" 222"}}</view></view><block wx:if="{{meeting.type!=='线上会议'}}"><view class="place"><image class="icon" src="../../static/icon-16.png" mode="widthFix"></image><text>{{meeting.place+" 333"}}</text></view></block></view><view class="desc">{{metting.desc}}</view><block wx:if="{{meeting.type=='线上会议'}}"><button class="btn">进入</button></block></view></view></block></view></block></view>